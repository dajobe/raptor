@prefix mkr: <http://mkrmke.net/ns/> ;

dbopen
    iss action ;
    
ke
    has dbopen = {
        _:genid15
            has agent = ke, 
            od (mkr.ho, hierarchy, gdbm, existent) ;
        }, 
    has print = {
        _:genid1
            has agent = ke, 
            od "The quick brown fox jumped over the lazy dog" ;
        }, 
    has read = {
        _:genid4
            has agent = ke, 
            from relation.mkr ;
        }, 
    has select = {
        _:genid7
            has agent = ke, 
            from relation.mkr, 
            od ($1, $3) ;
        } ;
    
print
    iss action ;
    
read
    iss action ;
    
select
    iss action ;
    
sh
    has wc = {
        _:genid12
            has agent = sh, 
            od *.mkr ;
        } ;
    
wc
