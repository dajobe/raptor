@base <http://www.w3.org/2001/sw/DataAccess/df1/tests/command.mkr> ;
@prefix rdf: <../../../../../1999/02/22-rdf-syntax-ns#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;

dbopen
    iss action ;
    
ke
    has dbopen = {
        _:genid15
            has agent = ke, 
            od (<http://mkrmke.net/ns/mkr.ho>, hierarchy, gdbm, existent) ;
        }, 
    has print = {
        _:genid1
            has agent = ke, 
            od "The quick brown fox jumped over the lazy dog" ;
        }, 
    has read = {
        _:genid4
            has agent = ke, 
            from <http://mkrmke.net/ns/relation.mkr> ;
        }, 
    has select = {
        _:genid7
            has agent = ke, 
            from <http://mkrmke.net/ns/relation.mkr>, 
            od (<http://mkrmke.net/ns/$1>, <http://mkrmke.net/ns/$3>) ;
        } ;
    
print
    iss action ;
    
read
    iss action ;
    
select
    iss action ;
    
sh
    has wc = {
        _:genid12
            has agent = sh, 
            od <http://mkrmke.net/ns/*.mkr> ;
        } ;
    
wc
    iss action ;
    
