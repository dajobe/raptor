@base <http://www.w3.org/2001/sw/DataAccess/df1/tests/command.mkr> ;
@prefix rdf: <../../../../../1999/02/22-rdf-syntax-ns#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;

dbopen
    iss action ;
    
ke
    has dbopen = {
        has agent = ke, 
        od (mkr.ho, hierarchy, gdbm, existent) ;
        }, 
    has print = {
        has agent = ke, 
        od ("The quick brown fox jumped over the lazy dog") ;
        }, 
    has read = {
        has agent = ke, 
        has from = (relation.mkr) ;
        }, 
    has select = {
        has agent = ke, 
        has from = (relation.mkr), 
        od (<http://mkrmke.net/ns/$1>, <http://mkrmke.net/ns/$3>) ;
        } ;
    
print
    iss action ;
    
read
    iss action ;
    
select
    iss action ;
    
sh
    has wc = {
        has agent = sh, 
        od (<http://mkrmke.net/ns/*.mkr>) ;
        } ;
    
wc
    iss action ;
    
