     ;

entity
    has isc = <http://mkrmke.net/ns/?z>, 
        <http://mkrmke.net/ns/isc*> = <http://mkrmke.net/ns/?z>
     ;

u:Garfield
    isa [ex:cat, <http://mkrmke.net/ns/?x>]
     ;

