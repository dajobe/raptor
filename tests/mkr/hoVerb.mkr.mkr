@base <http://www.w3.org/2001/sw/DataAccess/df1/tests/hoVerb.mkr> ;
@prefix rdf: <../../../../../1999/02/22-rdf-syntax-ns#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;
@prefix u: <http://mkrmke.net/ns/unit/> ;
@prefix rdfs: <../../../../../2000/01/rdf-schema#> ;
@prefix ex: <http://example.org/ex#> ;

ex:Garfield
    isa <http://mkrmke.net/ns/?x>, 
    <http://mkrmke.net/ns/isa*> = <http://mkrmke.net/ns/?y>, 
    isu <http://mkrmke.net/ns/?x>
     ;

ex:Odie
    isu ex:dog
     ;

ex:animal
    has isc = [ex:cat, ex:dog]
     ;

ex:person
    isa ex:homosapien
     ;

<http://mkrmke.net/ns/$top>
