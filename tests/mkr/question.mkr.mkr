@base <http://www.w3.org/2001/sw/DataAccess/df1/tests/question.mkr> ;
@prefix rdf: <../../../../../1999/02/22-rdf-syntax-ns#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;
@prefix U: <http://mkrmke.net/ns/universe/> ;
@prefix u: <http://mkrmke.net/ns/unit/> ;

?s
    has ?p = ?o, 
    isa ?o, 
    isa* entity ;
    
animal
    isa entity ;
    
cat
    has ?p = entity, 
    isa animal ;
    
dog
    isa animal ;
    
entity
    isc* ? ;
    
person
    isa entity ;
    
unit/Bob_McCullough
    isa person ;
    
unit/Dick_McCullough
    isa person ;
    
unit/Garfield
    isa cat, 
    isa* ?o ;
    
unit/Odie
    isa dog ;
    
