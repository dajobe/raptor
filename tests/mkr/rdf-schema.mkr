@base <http://www.w3.org/1999/02/22-rdf-syntax-ns> ;
@prefix rdf: <#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;
@prefix : <../../2000/01/rdf-schema#> ;

rdf:Alt
    isu :Class, 
    has :label = ["Alt"@en, "Choix"@fr], 
        :subClassOf = :Container ;
    
rdf:Bag
    isu :Class, 
    has :label = ["Bag"@en, "Ensemble"@fr], 
        :subClassOf = :Container ;
    
rdf:Property
    isu :Class, 
    has :comment = "The concept of a property.", 
        :label = ["Property"@en, "Propriété"@fr], 
        :subClassOf = :Resource ;
    
rdf:Seq
    isu :Class, 
    has :label = ["Sequence"@en, "Séquence"@fr], 
        :subClassOf = :Container ;
    
rdf:Statement
    isu :Class, 
    has :comment = "This represents the set of reified statements.", 
        :label = ["Déclaration"@fr, "Statement"@en], 
        :subClassOf = :Resource ;
    
rdf:object
    isu rdf:Property, 
    has :domain = rdf:Statement, 
        :label = ["object"@en, "objet"@fr] ;
    
rdf:predicate
    isu rdf:Property, 
    has :domain = rdf:Statement, 
        :label = ["predicate"@en, "prédicat"@fr], 
        :range = rdf:Property ;
    
rdf:subject
    isu rdf:Property, 
    has :domain = rdf:Statement, 
        :label = ["subject"@en, "sujet"@fr], 
        :range = :Resource ;
    
rdf:type
    isu rdf:Property, 
    has :comment = "Indicates membership of a class", 
        :label = ["type"@en, "type"@fr], 
        :range = :Class ;
    
rdf:value
    isu rdf:Property, 
    has :label = ["object"@en, "value"@fr] ;
    
<../../2000/01/rdf-schema#>
    has :seeAlso = <../../2000/01/rdf-schema-more> ;
    
:Class
    isu :Class, 
    has :comment = "The concept of Class", 
        :label = ["Class"@en, "Classe"@fr], 
        :subClassOf = :Resource ;
    
:ConstraintProperty
    isu :Class, 
    has :comment = "Properties used to express RDF Schema constraints.", 
        :label = ["ConstraintProperty"@en, "PropriétéContrainte"@fr], 
        :subClassOf = [rdf:Property, :ConstraintResource] ;
    
:ConstraintResource
    isu :Class, 
    has :comment = "Resources used to express RDF Schema constraints.", 
        :label = ["ConstraintResource"@en, "RessourceContrainte"@fr], 
        :subClassOf = :Resource ;
    
:Container
    isu :Class, 
    has :comment = "This represents the set Containers.", 
        :label = ["Container"@en, "Enveloppe"@fr], 
        :subClassOf = :Resource ;
    
:ContainerMembershipProperty
    isu :Class, 
    has :label = "ContainerMembershipProperty"@en, 
        :subClassOf = rdf:Property ;
    
:Literal
    isu :Class, 
    has :comment = "This represents the set of atomic values, eg. textual strings.", 
        :label = ["Literal"@en, "Littéral"@fr] ;
    
:Resource
    isu :Class, 
    has :comment = "The most general class", 
        :label = ["Resource"@en, "Ressource"@fr] ;
    
:comment
    isu rdf:Property, 
    has :comment = "Use this for descriptions", 
        :domain = :Resource, 
        :label = ["comment"@en, "commentaire"@fr], 
        :range = :Literal ;
    
:domain
    isu :ConstraintProperty, 
    has :comment = "This is how we associate a class with properties that its instances can have", 
        :label = ["domain"@en, "domaine"@fr] ;
    
:isDefinedBy
    isu rdf:Property, 
    has :comment = "Indicates a resource containing and defining the subject resource.", 
        :domain = :Resource, 
        :label = ["esDéfiniPar"@fr, "isDefinedBy"@en], 
        :range = :Resource, 
        :subPropertyOf = :seeAlso ;
    
:label
    isu rdf:Property, 
    has :comment = "Provides a human-readable version of a resource name.", 
        :domain = :Resource, 
        :label = ["label"@en, "label"@fr], 
        :range = :Literal ;
    
:range
    isu :ConstraintProperty, 
    has :comment = "Properties that can be used in a schema to provide constraints", 
        :domain = rdf:Property, 
        :label = ["range"@en, "étendue"@fr], 
        :range = :Class ;
    
:seeAlso
    isu rdf:Property, 
    has :comment = "Indicates a resource that provides information about the subject resource.", 
        :domain = :Resource, 
        :label = ["seeAlso"@en, "voirAussi"@fr], 
        :range = :Resource ;
    
:subClassOf
    isu rdf:Property, 
    has :comment = "Indicates membership of a class", 
        :domain = :Class, 
        :label = ["sousClasseDe"@fr, "subClassOf"@en], 
        :range = :Class ;
    
:subPropertyOf
    isu rdf:Property, 
    has :comment = "Indicates specialization of properties", 
        :domain = rdf:Property, 
        :label = ["sousPropriétéDe"@fr, "subPropertyOf"@en], 
        :range = rdf:Property ;
    
