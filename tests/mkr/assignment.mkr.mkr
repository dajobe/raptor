@base <http://www.w3.org/2001/sw/DataAccess/df1/tests/assignment.mkr> ;
@prefix rdf: <../../../../../1999/02/22-rdf-syntax-ns#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;

ke
    has print = [{
        _:genid1
            has agent = ke, 
            od (<http://mkrmke.net/ns/$x>, <http://mkrmke.net/ns/$y>, <http://mkrmke.net/ns/$z>) ;
        }, {
        _:genid5
            has agent = ke, 
            od ("$x, $y, $z") ;
        }] ;
    
print
    iss action ;
    
x
    has <http://mkrmke.net/ns/:=> = 10 ;
    
y
    has <http://mkrmke.net/ns/:=> = _:5 ;
    
z
    has <http://mkrmke.net/ns/:=> = "the big red one" ;
    
