@base <http://www.w3.org/2000/01/rdf-schema> ;
@prefix rdf: <../../1999/02/22-rdf-syntax-ns#> ;
@prefix mkr: <http://mkrmke.net/ns/> ;
@prefix : <#> ;

rdf:Alt
    isu :Class, 
    has :comment = "A collection of alternatives."@en, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "Alt"@en, 
        :subClassOf = :Container
     ;

rdf:Bag
    isu :Class, 
    has :comment = "An unordered collection."@en, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "Bag"@en, 
        :subClassOf = :Container
     ;

rdf:Property
    isu :Class, 
    has :comment = "The concept of a property.", 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "Property"@en, 
        :subClassOf = :Resource
     ;

rdf:Seq
    isu :Class, 
    has :comment = "An ordered collection."@en, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "Seq"@en, 
        :subClassOf = :Container
     ;

rdf:Statement
    isu :Class, 
    has :comment = "The class of RDF statements.", 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "Statement"@en, 
        :subClassOf = :Resource
     ;

rdf:object
    isu rdf:Property, 
    has :comment = "The object of an RDF statement.", 
        :domain = rdf:Statement, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "object"@en
     ;

rdf:predicate
    isu rdf:Property, 
    has :comment = "the predicate of an RDF statement.", 
        :domain = rdf:Statement, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "predicate"@en, 
        :range = rdf:Property
     ;

rdf:subject
    isu rdf:Property, 
    has :comment = "The subject of an RDF statement.", 
        :domain = rdf:Statement, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "subject"@en, 
        :range = :Resource
     ;

rdf:type
    isu rdf:Property, 
    has :comment = "Indicates membership of a class", 
        :domain = :Resource, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "type"@en, 
        :range = :Class
     ;

rdf:value
    isu rdf:Property, 
    has :comment = "Identifies the principal value (usually a string) of a property when the property value is a structured resource", 
        :domain = :Resource, 
        :isDefinedBy = <../../1999/02/22-rdf-syntax-ns#>, 
        :label = "value"@en
     ;

<#>
    has :seeAlso = <rdf-schema-more>
     ;

:Class
    isu :Class, 
    has :comment = "The concept of Class", 
        :isDefinedBy = <#>, 
        :label = "Class"@en, 
        :subClassOf = :Resource
     ;

:Container
    isu :Class, 
    has :comment = "This represents the set Containers.", 
        :isDefinedBy = <#>, 
        :label = "Container"@en, 
        :subClassOf = :Resource
     ;

:ContainerMembershipProperty
    isu :Class, 
    has :comment = "The container membership properties, rdf:1, rdf:2, ..., all of which are sub-properties of 'member'.", 
        :isDefinedBy = <#>, 
        :label = "ContainerMembershipProperty"@en, 
        :subClassOf = rdf:Property
     ;

:Literal
    isu :Class, 
    has :comment = "This represents the set of atomic values, eg. textual strings.", 
        :isDefinedBy = <#>, 
        :label = "Literal"@en
     ;

:Resource
    isu :Class, 
    has :comment = "The class resource, everything.", 
        :isDefinedBy = <#>, 
        :label = "Resource"@en
     ;

:comment
    isu rdf:Property, 
    has :comment = "Use this for descriptions", 
        :domain = :Resource, 
        :isDefinedBy = <#>, 
        :label = "comment"@en, 
        :range = :Literal
     ;

:domain
    isu rdf:Property, 
    has :comment = "A domain class for a property type", 
        :domain = rdf:Property, 
        :isDefinedBy = <#>, 
        :label = "domain"@en, 
        :range = :Class
     ;

:isDefinedBy
    isu rdf:Property, 
    has :comment = "Indicates the namespace of a resource", 
        :domain = :Resource, 
        :isDefinedBy = <#>, 
        :label = "isDefinedBy"@en, 
        :range = :Resource, 
        :subPropertyOf = :seeAlso
     ;

:label
    isu rdf:Property, 
    has :comment = "Provides a human-readable version of a resource name.", 
        :domain = :Resource, 
        :isDefinedBy = <#>, 
        :label = "label"@en, 
        :range = :Literal
     ;

:member
    isu rdf:Property, 
    has :comment = "a member of a container", 
        :domain = :Container, 
        :isDefinedBy = <#>, 
        :label = "member"@en
     ;

:range
    isu rdf:Property, 
    has :comment = "A range class for a property type", 
        :domain = rdf:Property, 
        :isDefinedBy = <#>, 
        :label = "range"@en, 
        :range = :Class
     ;

:seeAlso
    isu rdf:Property, 
    has :comment = "A resource that provides information about the subject resource", 
        :domain = :Resource, 
        :isDefinedBy = <#>, 
        :label = "seeAlso"@en, 
        :range = :Resource
     ;

:subClassOf
    isu rdf:Property, 
    has :comment = "Indicates membership of a class", 
        :domain = :Class, 
        :isDefinedBy = <#>, 
        :label = "subClassOf"@en, 
        :range = :Class
     ;

:subPropertyOf
    isu rdf:Property, 
    has :comment = "Indicates specialization of properties", 
        :domain = rdf:Property, 
        :isDefinedBy = <#>, 
        :label = "subPropertyOf"@en, 
        :range = rdf:Property
     ;

